<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="CustomModeView">
		<attr name="selectedColor" format="integer"/>
		<attr name="deselectedColor" format="integer"/>
		<attr name="fontSizeBig" format="float"/>
		<attr name="fontSizeSmall" format="float"/>
		<attr name="selected" format="boolean"/>
		<attr name="selectedImage" format="integer"/>
		<attr name="deselectedImage" format="integer"/>
	</declare-styleable>
	<declare-styleable name="FanView">
        <attr name="menuSize" format="dimension"/>
        <attr name="fanOrientation" format="enum">
            <enum name="horizontal" value="0"></enum>
            <enum name="vertical" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>