<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="PanelButton">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#008</item>
        <item name="android:layout_width">@dimen/panel_button_size</item>
        <item name="android:minWidth">@dimen/panel_button_size</item>
        <item name="android:layout_height">@dimen/panel_button_size</item>
        <item name="android:minHeight">@dimen/panel_button_size</item>
        <item name="android:layout_marginBottom">9dip</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    
    <style name="FocusDialog"   parent="@android:style/Theme.Dialog">
  		<item name="android:textColor">#00FF00</item>
  		<item name="android:background">#bbe031</item>
  		<item name="android:typeface">monospace</item>
  		<item name="android:textSize">20dp</item>
		<item name="android:gravity">center_vertical|center_horizontal</item>
 	</style>
 	
    <style name="PhotoGridLayout">
        <item name="android:drawSelectorOnTop">true</item>
        <item name="android:listSelector">@drawable/photogrid_list_selector</item>
    </style>
    
    <style name="GalleryTheme" parent="@android:style/Theme.Holo">
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:actionBarStyle">@style/TranslucentDarkActionBar</item>
    </style>

    <style name="GalleryTheme.FullScreen" />

    <style name="TranslucentDarkActionBar" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">#99000000</item>
    </style>
    
    <style name="Fan_Menu_SeekBar">
        <item name="android:paddingLeft">50dp</item>
        <item name="android:paddingRight">50dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>
</resources>